// 3.5.1 Repeated_word_detection_program
// March 7, 2017
// Amandy

#include <iostream>
#include <string>
using namespace std;

int main()
{
    
 string previous =" ";
 string current;
  
  while (cin>>current){
      
   
        if (previous==current)
                 cout <<"repeated word:" << current << '\n';
                previous = current;
                
  }

}
